/* pages/scan/scan.wxss */
.withdrawal {
	width: 360rpx;
	height: 88rpx;
	line-height: 88rpx;
	box-shadow: 0px 5px 15px 0px rgba(2, 174, 255, 0.36);
	border-radius: 40px;
	background: linear-gradient(0deg, #3F8DFC 0%, #76D3FE 99%);
	position: absolute;
	transform: translateX(-50%);
	left: 50%;
	bottom: 50rpx;
}
